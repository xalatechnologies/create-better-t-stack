{
  "name": "nextjs-config",
  "description": "Next.js configuration with Norwegian compliance and performance optimizations",
  "category": "config",
  "tags": [
    "nextjs",
    "config",
    "typescript",
    "i18n",
    "security",
    "performance",
    "norwegian-compliance",
    "localization",
    "pwa"
  ],
  "requiredContext": [
    "description"
  ],
  "optionalContext": [
    "typescript",
    "experimental",
    "serverComponents",
    "appDir",
    "typedRoutes",
    "optimizePackageImports",
    "serverComponentsExternalPackages",
    "i18n",
    "domains",
    "images",
    "compiler",
    "removeConsole",
    "reactRemoveProperties",
    "styledComponents",
    "poweredByHeader",
    "compress",
    "reactStrictMode",
    "swcMinify",
    "eslint",
    "headers",
    "csp",
    "hsts",
    "staticHeaders",
    "apiHeaders",
    "cors",
    "redirects",
    "rewrites",
    "webpack",
    "bundleAnalyzer",
    "svgr",
    "aliasOptimization",
    "nodePolyfills",
    "env",
    "publicRuntimeConfig",
    "serverRuntimeConfig",
    "output",
    "distDir",
    "generateEtags",
    "pageExtensions",
    "trailingSlash",
    "basePath",
    "assetPrefix",
    "analytics",
    "pwa",
    "moduleExports"
  ],
  "dependencies": [
    "next"
  ],
  "platform": [
    "nextjs"
  ],
  "version": "1.0.0",
  "author": "Xala Technologies",
  "examples": [
    {
      "name": "Basic Norwegian App Config",
      "context": {
        "description": "Next.js configuration for Norwegian compliance application",
        "typescript": true,
        "i18n": true,
        "domains": {
          "no": "app.example.no",
          "com": "app.example.com",
          "fr": "app.example.fr"
        },
        "images": {
          "domains": ["cdn.example.com", "images.example.no"],
          "deviceSizes": [640, 768, 1024, 1280, 1600],
          "imageSizes": [16, 32, 48, 64, 96, 128, 256, 384],
          "cacheTTL": 86400
        },
        "compiler": {
          "removeConsole": true,
          "reactRemoveProperties": true
        },
        "poweredByHeader": false,
        "compress": true,
        "reactStrictMode": true,
        "swcMinify": true,
        "eslint": {
          "dirs": ["pages", "components", "lib"]
        },
        "headers": true,
        "csp": true,
        "hsts": true,
        "staticHeaders": true,
        "cors": {
          "origin": "https://app.example.no"
        },
        "moduleExports": false
      }
    },
    {
      "name": "Enterprise Dashboard Config",
      "context": {
        "description": "Enterprise Next.js configuration with advanced features",
        "typescript": true,
        "experimental": {
          "serverComponents": true,
          "appDir": true,
          "typedRoutes": true,
          "optimizePackageImports": true,
          "serverComponentsExternalPackages": true
        },
        "i18n": true,
        "images": {
          "remotePatterns": [
            {
              "protocol": "https",
              "hostname": "cdn.example.com",
              "pathname": "/images/**"
            }
          ],
          "dangerouslyAllowSVG": true,
          "deviceSizes": [640, 768, 1024, 1280, 1600],
          "imageSizes": [16, 32, 48, 64, 96, 128, 256, 384],
          "cacheTTL": 86400
        },
        "compiler": {
          "removeConsole": true,
          "reactRemoveProperties": true
        },
        "headers": true,
        "csp": true,
        "hsts": true,
        "apiHeaders": true,
        "cors": {
          "origin": "https://dashboard.example.no"
        },
        "webpack": {
          "bundleAnalyzer": true,
          "svgr": true,
          "aliasOptimization": true,
          "nodePolyfills": true
        },
        "env": [
          {
            "name": "DATABASE_URL"
          },
          {
            "name": "NEXTAUTH_SECRET"
          }
        ],
        "publicRuntimeConfig": [
          {
            "name": "API_URL",
            "envVar": "NEXT_PUBLIC_API_URL"
          }
        ],
        "serverRuntimeConfig": [
          {
            "name": "DATABASE_URL",
            "envVar": "DATABASE_URL"
          }
        ],
        "analytics": true,
        "moduleExports": false
      }
    }
  ]
}