{
  "name": "nextjs-page",
  "description": "Next.js page component with SSR, SEO, and Norwegian compliance",
  "category": "page",
  "tags": [
    "nextjs",
    "page",
    "ssr",
    "ssg",
    "seo",
    "typescript",
    "norwegian-compliance",
    "accessibility",
    "localization"
  ],
  "requiredContext": [
    "name",
    "description"
  ],
  "optionalContext": [
    "typescript",
    "localization",
    "layout",
    "components",
    "sections",
    "actions",
    "props",
    "staticGeneration",
    "serverSideRendering",
    "staticProps",
    "serverProps",
    "staticDataSources",
    "serverDataSources",
    "hasHeader",
    "hasContent",
    "hasActions",
    "hasState",
    "hasEffects",
    "hasKeywords",
    "authentication",
    "analytics",
    "seo",
    "accessibility",
    "compliance",
    "revalidate"
  ],
  "dependencies": [
    "next",
    "react",
    "@types/react",
    "@types/next"
  ],
  "platform": [
    "nextjs"
  ],
  "version": "1.0.0",
  "author": "Xala Technologies",
  "examples": [
    {
      "name": "Simple Static Page",
      "context": {
        "name": "About",
        "description": "About us page with company information",
        "typescript": true,
        "localization": true,
        "staticGeneration": true,
        "hasHeader": true,
        "hasContent": true,
        "seo": {
          "title": "About Us",
          "description": "Learn more about our company and mission",
          "keywords": "about, company, mission, values"
        },
        "sections": [
          {
            "name": "mission",
            "title": "Our Mission",
            "components": [
              {
                "name": "Text",
                "props": [
                  {
                    "name": "content",
                    "value": "t('about.mission.content')"
                  }
                ]
              }
            ]
          }
        ],
        "accessibility": {
          "skipLink": true,
          "announcements": true
        },
        "compliance": {
          "nsm": {
            "classification": "OPEN"
          },
          "gdpr": true,
          "wcag": "AAA"
        }
      }
    },
    {
      "name": "Dynamic User Dashboard",
      "context": {
        "name": "Dashboard",
        "description": "User dashboard with real-time data",
        "typescript": true,
        "localization": true,
        "serverSideRendering": true,
        "authentication": true,
        "hasHeader": true,
        "hasContent": true,
        "hasActions": true,
        "hasState": true,
        "hasEffects": true,
        "analytics": true,
        "layout": {
          "name": "AppLayout",
          "props": [
            {
              "name": "sidebar",
              "value": "true"
            }
          ]
        },
        "serverProps": [
          {
            "name": "userData",
            "source": "user",
            "fallbackValue": "null"
          },
          {
            "name": "dashboardData",
            "source": "dashboardData",
            "fallbackValue": "[]"
          }
        ],
        "serverDataSources": [
          {
            "name": "dashboardData"
          }
        ],
        "actions": [
          {
            "label": "Refresh Data",
            "onClick": "handleRefresh",
            "accessibility": {
              "ariaLabel": "Refresh dashboard data"
            }
          }
        ],
        "seo": {
          "title": "Dashboard",
          "description": "User dashboard with personalized data and analytics"
        },
        "accessibility": {
          "skipLink": true,
          "announcements": true
        },
        "compliance": {
          "nsm": {
            "classification": "RESTRICTED"
          },
          "gdpr": true,
          "wcag": "AAA"
        }
      }
    }
  ]
}