    "jsdom": "^23.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0",
    "vite": "^4.5.0",
    "vitest": "^0.34.0",
    "@vitest/ui": "^0.34.0",
    "vue-tsc": "^1.8.0",
