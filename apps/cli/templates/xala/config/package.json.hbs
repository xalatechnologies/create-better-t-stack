{
  "name": "{{projectName}}",
  "version": "{{version}}",
  "description": "{{description}} - Generated with Xaheen CLI using Xala UI System v5",
  "_compliance_rules": {
    "mandatory": [
      "NO raw HTML elements (div, span, p, h1-h6, button, input, etc.)",
      "ONLY semantic components from @xala-technologies/ui-system",
      "NO hardcoded styling (no style={{}}, no arbitrary Tailwind values)",
      "MANDATORY design token usage for all colors, spacing, typography",
      "Enhanced 8pt Grid System - all spacing in 8px increments",
      "WCAG 2.2 AAA compliance for accessibility",
      "NO hardcoded user-facing text - ALL text must use t() function",
      "MANDATORY localization: English, Norwegian BokmÃ¥l, French, Arabic",
      "Explicit TypeScript return types (no 'any' types)",
      "SOLID principles and component composition",
      "Maximum 200 lines per file, 20 lines per function"
    ]
  },
  "scripts": {
    "dev": "{{#if framework}}{{#eq framework 'next'}}next dev{{/eq}}{{#eq framework 'vite'}}vite{{/eq}}{{#eq framework 'remix'}}remix dev{{/eq}}{{else}}next dev{{/if}}",
    "build": "{{#if framework}}{{#eq framework 'next'}}next build{{/eq}}{{#eq framework 'vite'}}vite build{{/eq}}{{#eq framework 'remix'}}remix build{{/eq}}{{else}}next build{{/if}}",
    "start": "{{#if framework}}{{#eq framework 'next'}}next start{{/eq}}{{#eq framework 'vite'}}vite preview{{/eq}}{{#eq framework 'remix'}}remix start{{/eq}}{{else}}next start{{/if}}",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "{{#if testing}}{{#eq testing 'vitest'}}vitest{{/eq}}{{#eq testing 'jest'}}jest{{/eq}}{{else}}vitest{{/if}}",
    "test:ui": "{{#if testing}}{{#eq testing 'vitest'}}vitest --ui{{/eq}}{{#eq testing 'jest'}}jest --watch{{/eq}}{{else}}vitest --ui{{/if}}",
    "test:coverage": "{{#if testing}}{{#eq testing 'vitest'}}vitest --coverage{{/eq}}{{#eq testing 'jest'}}jest --coverage{{/eq}}{{else}}vitest --coverage{{/if}}",
    "storybook": "{{#if storybook}}storybook dev -p 6006{{/if}}",
    "build-storybook": "{{#if storybook}}storybook build{{/if}}",
    "accessibility:test": "{{#if accessibility}}axe-core{{/if}}",
    "accessibility:audit": "{{#if accessibility}}lighthouse --only=accessibility{{/if}}"
  },
  "dependencies": {
    {{#if framework}}
    {{#eq framework 'next'}}
    "next": "^14.0.0",
    {{/eq}}
    {{#eq framework 'remix'}}
    "@remix-run/node": "^2.0.0",
    "@remix-run/react": "^2.0.0",
    "@remix-run/serve": "^2.0.0",
    {{/eq}}
    {{#eq framework 'vite'}}
    "vite": "^5.0.0",
    {{/eq}}
    {{else}}
    "next": "^14.0.0",
    {{/if}}
    
    {{#if typescript}}
    "typescript": "^5.0.0",
    {{/if}}
    
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    
    {{! Xala UI System - Main dependency }}
    "@xala-technologies/ui-system": "^5.0.0",
    
    {{! Design tokens and theming }}
    "@xala-technologies/design-tokens": "^2.0.0",
    "@xala-technologies/theme-provider": "^1.0.0",
    
    {{! Norwegian localization }}
    {{#if localization}}
    "next-intl": "^3.0.0",
    "react-intl": "^6.0.0",
    {{/if}}
    
    {{! Accessibility utilities }}
    {{#if accessibility}}
    "@radix-ui/react-focus-scope": "^1.0.0",
    "@radix-ui/react-focus-guards": "^1.0.0",
    "@radix-ui/react-id": "^1.0.0",
    "@radix-ui/react-use-escape-keydown": "^1.0.0",
    "react-aria": "^3.0.0",
    "react-aria-live": "^3.0.0",
    {{/if}}
    
    {{! Form handling }}
    {{#if forms}}
    "react-hook-form": "^7.0.0",
    "@hookform/resolvers": "^3.0.0",
    "zod": "^3.0.0",
    {{/if}}
    
    {{! State management }}
    {{#if stateManagement}}
    {{#eq stateManagement 'zustand'}}
    "zustand": "^4.0.0",
    {{/eq}}
    {{#eq stateManagement 'redux'}}
    "@reduxjs/toolkit": "^2.0.0",
    "react-redux": "^9.0.0",
    {{/eq}}
    {{#eq stateManagement 'jotai'}}
    "jotai": "^2.0.0",
    {{/eq}}
    {{/if}}
    
    {{! Styling }}
    "tailwindcss": "^3.4.0",
    "autoprefixer": "^10.0.0",
    "postcss": "^8.0.0",
    "clsx": "^2.0.0",
    "tailwind-merge": "^2.0.0",
    "class-variance-authority": "^0.7.0",
    
    {{! Icons }}
    {{#if icons}}
    {{#eq icons 'lucide'}}
    "lucide-react": "^0.300.0",
    {{/eq}}
    {{#eq icons 'heroicons'}}
    "@heroicons/react": "^2.0.0",
    {{/eq}}
    {{#eq icons 'tabler'}}
    "@tabler/icons-react": "^2.0.0",
    {{/eq}}
    {{else}}
    "lucide-react": "^0.300.0",
    {{/if}}
    
    {{! Animation }}
    {{#if animations}}
    "framer-motion": "^10.0.0",
    "react-spring": "^9.0.0",
    {{/if}}
    
    {{! Date handling }}
    {{#if dates}}
    "date-fns": "^3.0.0",
    "date-fns-tz": "^2.0.0",
    {{/if}}
    
    {{! Utilities }}
    "react-use": "^17.0.0",
    "usehooks-ts": "^2.0.0"
  },
  "devDependencies": {
    {{#if typescript}}
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "typescript": "^5.0.0",
    {{/if}}
    
    {{! ESLint configuration }}
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint-plugin-react": "^7.0.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "eslint-plugin-jsx-a11y": "^6.0.0",
    "eslint-plugin-import": "^2.0.0",
    
    {{! Prettier }}
    "prettier": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.5.0",
    
    {{! Testing }}
    {{#if testing}}
    {{#eq testing 'vitest'}}
    "vitest": "^1.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "jsdom": "^23.0.0",
    {{/eq}}
    {{#eq testing 'jest'}}
    "jest": "^29.0.0",
    "@testing-library/jest-dom": "^6.0.0",
    "jest-environment-jsdom": "^29.0.0",
    {{/eq}}
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.0.0",
    "happy-dom": "^12.0.0",
    {{/if}}
    
    {{! Accessibility testing }}
    {{#if accessibility}}
    "@axe-core/react": "^4.0.0",
    "axe-core": "^4.0.0",
    "lighthouse": "^11.0.0",
    "pa11y": "^8.0.0",
    {{/if}}
    
    {{! Storybook }}
    {{#if storybook}}
    "@storybook/react": "^7.0.0",
    "@storybook/react-vite": "^7.0.0",
    "@storybook/addon-essentials": "^7.0.0",
    "@storybook/addon-a11y": "^7.0.0",
    "@storybook/addon-docs": "^7.0.0",
    {{/if}}
    
    {{! Build tools }}
    "cross-env": "^7.0.0",
    "rimraf": "^5.0.0",
    
    {{! Development utilities }}
    "concurrently": "^8.0.0",
    "nodemon": "^3.0.0"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "next/core-web-vitals",
      "plugin:@typescript-eslint/recommended",
      "plugin:jsx-a11y/recommended"
    ],
    "rules": {
      "jsx-a11y/anchor-is-valid": "error",
      "jsx-a11y/aria-props": "error",
      "jsx-a11y/aria-proptypes": "error",
      "jsx-a11y/aria-unsupported-elements": "error",
      "jsx-a11y/role-has-required-aria-props": "error",
      "jsx-a11y/role-supports-aria-props": "error"
    }
  },
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 100,
    "tabWidth": 2,
    "useTabs": false,
    "plugins": ["prettier-plugin-tailwindcss"]
  },
  "keywords": [
    "react",
    "typescript",
    "xala-ui",
    "design-system",
    "accessibility",
    "wcag",
    "norwegian",
    "gdpr",
    "ui-components"
  ],
  "author": "{{author}}",
  "license": "{{license}}"
}
