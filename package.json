{
  "name": "create-better-t-stack",
  "version": "1.0.0",
  "description": "CLI tool to scaffold Better-T Stack projects",
  "type": "module",
  "bin": {
    "create-better-t-stack": "./dist/index.js"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "check": "biome check --write .",
    "test": "vitest run",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist",
    "templates"
  ],
  "dependencies": {
    "@inquirer/prompts": "^7.3.1",
    "chalk": "^5.3.0",
    "commander": "^13.1.0",
    "execa": "^8.0.1",
    "fs-extra": "^11.2.0",
    "gradient-string": "^3.0.0",
    "ora": "^7.0.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.10.5",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  }
}
